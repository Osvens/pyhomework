def my_self (name, surname, year, city, email, telephone):
    return  ' '.join([name, surname, year, city, email, telephone])

print(my_self((name = 'Vlad', surname = 'Eremich', year = '23', city = 'SPb', email = 'osvens@mail.ru', telephone = 'no'))

