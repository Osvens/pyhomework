def my_func(arg_1, arg_2, arg_3):
    if arg_1 >= arg_3 and arg_2 >= arg_3:
        return arg_1 + arg_2
    elif arg_1 > arg_2 and arg_1 < arg_3:
        return arg_1 + arg_3
    else:
        return arg_3 + arg_2

print(f'res {my_func(int(input("Введите первый аргумент")), int(input("Введите второй аргумент")),  int(input("Введите третий аргумент")))}')


